	<body>
		<segment id="1" parent="43" relname="span">Seit dem 11. September ist alles anders ?</segment>
		<segment id="2" parent="1" relname="interpretation">Von wegen .</segment>
		<segment id="3" parent="22" relname="list">Bin Laden hat Amerika getroffen ,</segment>
		<segment id="4" parent="22" relname="list">die westliche Welt herausgefordert</segment>
		<segment id="5" parent="22" relname="list">und Krieg ausgelöst</segment>
		<segment id="6" parent="33" relname="span">- die deutsche Spaßkultur widersteht ihm .</segment>
		<segment id="7" parent="8" relname="preparation">Deutschland diskutiert :</segment>
		<segment id="8" parent="23" relname="span">Waren Veronas Tränen echt ?</segment>
		<segment id="9" parent="28" relname="span">Und Johannes B. Kerner gibt Interviews über seine Vermittlerrolle im Ehekrieg ,</segment>
		<segment id="10" parent="9" relname="e-elaboration">gegen die Bushs Medienauftritte alt aussehen .</segment>
		<segment id="11" parent="12" relname="attribution">Wer gehofft hatte ,</segment>
		<segment id="12" parent="29" relname="span">dass uns künftig wenigstens Veronas Tränen und Dieters Geschwätz auf dem Bildschirm erspart bleiben ,</segment>
		<segment id="13" parent="25" relname="span">sieht sich getäuscht .</segment>
		<segment id="14" parent="26" relname="span">Es muss wohl mehr passieren ,</segment>
		<segment id="15" parent="14" relname="circumstance">bevor der Boulevard sich von seinen Schießbudenfiguren trennt .</segment>
		<segment id="16" parent="17" relname="condition">Solange wir sie sehen , lesen , kaufen und darüber reden wollen ,</segment>
		<segment id="17" parent="27" relname="span">werden wir damit leben müssen .</segment>
		<segment id="18" parent="19" relname="evaluation-s">Einen Trost gibt es :</segment>
		<segment id="19" parent="30" relname="span">Veronas Tränen sind stärker als bin Ladens Schläfer .</segment>
		<segment id="20" parent="21" relname="evaluation-s">Berlusconi hat schon Recht :</segment>
		<segment id="21" parent="31" relname="span">Der Westen ist dem Islam überlegen .</segment>
